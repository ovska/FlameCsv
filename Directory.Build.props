<Project>
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <!-- <SuppressPreviewSDKWarning>true</SuppressPreviewSDKWarning> -->
        <LangVersion>preview</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <!-- TODO: better name for this -->
        <IncludeInternalsVisibleTo Condition="'$(IncludeInternalsVisibleTo)' == ''">true</IncludeInternalsVisibleTo>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)FlameCsv.snk</AssemblyOriginatorKeyFile>
        <FlameCsvPublicKey></FlameCsvPublicKey>
    </PropertyGroup>
    <ItemGroup>
        <Using Include="System.Numerics" />
    </ItemGroup>
</Project>
